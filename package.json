{
  "name": "nestjs-sample-server",
  "version": "1.3.100",
  "description": "Nestjs Sample back end built with NestJS and TypeScript",
  "author": "",
  "private": true,
  "license": "ISC",
  "scripts": {
    "run-email-preview": "ts-node-dev --rs --transpile-only --exit-child src/email-templates/email-preview-server.ts",
    "prebuild": "rimraf dist",
    "check-types": "tsc",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "generate:prisma-client": "prisma generate",
    "generate:prisma-migrate": "prisma migrate dev",
    "generate:prisma-migration": "prisma migrate dev --name",
    "create:prisma-migration": "prisma migrate dev --create-only --name",
    "deploy:migration": "prisma migrate deploy",
    "start": "nest start",
    "dev": "npm run watch:build",
    "watch:build": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "isready": "npm run check-types && npm run test && npm run-script build"
  },
  "dependencies": {
    "dd-trace": "^2.4.1",
    "@aws-sdk/client-sesv2": "3.51.0",
    "@nestjs/axios": "0.0.7",
    "@nestjs/common": "8.4.4",
    "@nestjs/config": "1.2.1",
    "@nestjs/core": "8.4.3",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-express": "8.4.4",
    "@nestjs/serve-static": "2.2.2",
    "@okta/jwt-verifier": "2.3.0",
    "@okta/okta-sdk-nodejs": "5.0.0",
    "@sendgrid/mail": "7.6.2",
    "@prisma/client": "3.12.0",
    "@types/async": "3.2.13",
    "async": "3.2.3",
    "axios": "0.26.1",
    "cache-manager": "3.6.0",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.2",
    "crypto": "1.0.1",
    "express": "4.17.2",
    "express-basic-auth": "1.2.1",
    "hellosign-sdk": "2.0.0",
    "html-minifier": "4.0.0",
    "json2csv": "5.0.7",
    "jsonwebtoken": "8.5.1",
    "jwk-to-pem": "2.0.5",
    "jwt-decode": "3.1.2",
    "mjml": "4.10.4",
    "mjml-react": "2.0.7",
    "moment": "2.29.1",
    "moment-business-days": "1.2.0",
    "nestjs-pino": "1.4.0",
    "passport": "0.5.2",
    "passport-headerapikey": "1.2.2",
    "passport-http-bearer": "1.0.1",
    "pino-pretty": "7.6.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "reflect-metadata": "0.1.13",
    "request": "2.88.2",
    "rxjs": "7.5.5",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@aws-sdk/types": "3.25.0",
    "@nestjs/cli": "8.2.5",
    "@nestjs/schematics": "8.0.10",
    "@nestjs/swagger": "5.2.1",
    "@nestjs/testing": "8.4.4",
    "@next/eslint-plugin-next": "12.1.5",
    "@types/cache-manager": "3.4.3",
    "@types/express": "4.17.13",
    "@types/hellosign-sdk": "1.6.10",
    "@types/html-minifier": "4.0.2",
    "@types/jest": "27.0.2",
    "@types/json2csv": "5.0.3",
    "@types/jsonwebtoken": "8.5.8",
    "@types/jwk-to-pem": "2.0.1",
    "@types/mjml": "4.7.0",
    "@types/mjml-react": "2.0.4",
    "@types/multer": "1.4.7",
    "@types/node": "17.0.29",
    "@types/passport-http-bearer": "1.0.37",
    "@types/passport-strategy": "0.2.35",
    "@types/react": "18.0.8",
    "@types/styled-components": "5.1.25",
    "@types/supertest": "2.0.12",
    "@types/uuid": "8.3.4",
    "@types/validator": "13.7.0",
    "@types/webpack-env": "1.16.4",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "concurrently": "6.2.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "26.1.4",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.7",
    "prettier": "2.6.2",
    "prisma": "3.12.0",
    "run-script-webpack-plugin": "0.0.11",
    "supertest": "6.2.3",
    "swagger-ui-express": "4.3.0",
    "ts-jest": "27.0.7",
    "ts-loader": "9.2.6",
    "ts-node": "10.7.0",
    "ts-node-dev": "1.1.8",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.3.5",
    "watch": "1.0.2",
    "webpack": "5.68.0",
    "webpack-node-externals": "3.0.0"
  },
  "resolutions": {
    "@types/react": "18.0.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run check-types && npm run test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache",
      "git add --force"
    ],
    "*.{json,md,html}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.[jt]sx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.[jt]sx?"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
